<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="SpeedySchema">
      <section name="TypeMapper" type="SpeedySchema.Config.DataTypeMappingConfigurationSection, SpeedySchema" />
    </sectionGroup>
  </configSections>
  
  <connectionStrings>
    <add name="db" connectionString="xxx"/>
  </connectionStrings>

  <SpeedySchema>
    <TypeMapper>
      <DataTypes>
        <DataType UserType="text" SQLType="NVARCHAR" FieldLength="100"></DataType>
        <DataType UserType="string" SQLType="NVARCHAR" FieldLength="100"></DataType>
        <DataType UserType="word" SQLType="NVARCHAR" FieldLength="100"></DataType>
        <DataType UserType="datetime" SQLType="DateTime" FieldLength="-1"></DataType>
      </DataTypes>
    </TypeMapper>
  </SpeedySchema>
</configuration>